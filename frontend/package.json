{
  "version": "1.0.0",
  "name": "tokki-tok",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "http://blacktokki.github.io/tokki-tok",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "desktop": "expo-electron start",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "mobile:android": "expo build:android",
    "github": "expo build:web && gh-pages -d web-build",
    "desktop:mac": "electron-webpack && electron-builder --dir -c.compression=store -c.mac.identity=null",
    "desktop:windows": ".\\node_modules\\.bin\\electron-webpack.cmd && electron-builder --dir -c.compression=store",
    "mac": "electron-webpack && electron-builder --mac -c.compression=store",
    "windows": ".\\node_modules\\.bin\\electron-webpack.cmd && electron-builder --win -c.compression=store",
    "bot": "node scripts/bot.js",
    "env": "node scripts/env.js",
    "install:expo-cli": "npm i -g expo-cli@6.0.6"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@formatjs/intl": "^2.7.2",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "~5.8.10",
    "@react-navigation/stack": "~5.12.8",
    "axios": "^1.1.3",
    "expo": "~41.0.1",
    "expo-asset": "~8.3.1",
    "expo-build-properties": "^0.3.0",
    "expo-constants": "~10.1.3",
    "expo-font": "~9.1.0",
    "expo-linking": "~2.2.3",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-web-browser": "~9.1.0",
    "firebase": "^8.2.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-dotenv": "^3.4.7",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-hyperlink": "^0.0.22",
    "react-native-localize": "^3.0.0",
    "react-native-pager-view": "^5.0.12",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-tab-view": "^3.3.0",
    "react-native-web": "^0.17.1",
    "react-native-webrtc": "^1.92.2",
    "react-native-webrtc-web-shim": "^1.0.3",
    "react-native-webview": "11.2.3",
    "react-query": "^3.39.2",
    "react-use-websocket": "^4.2.0",
    "use-mobile-detect-hook": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@config-plugins/react-native-webrtc": "^1.0.0",
    "@expo/electron-adapter": "0.0.55",
    "@expo/webpack-config": "^0.13.1",
    "@types/lodash": "^4.14.170",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "concurrently": "^6.2.1",
    "electron": "^9.4.0",
    "electron-builder": "^22.11.7",
    "gh-pages": "^3.2.2",
    "jest-expo": "~41.0.0",
    "npm-link-shared": "^0.5.6",
    "typescript": "~4.0.0"
  },
  "build": {
    "appId": "com.electron.tokkitok",
    "mac": {
      "target": "default",
      "category": "public.app-category.business"
    },
    "dmg": {
      "title": "tokki-tok"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  },
  "private": true
}
